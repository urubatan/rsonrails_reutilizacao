<h1>Listing story_states</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Order</th>
    <th>User visible</th>
    <th>Project owner</th>
  </tr>

<% @story_states.each do |story_state| %>
  <tr>
    <td><%=h story_state.name %></td>
    <td><%=h story_state.order %></td>
    <td><%=h story_state.user_visible %></td>
    <td><%=h story_state.project_owner %></td>
    <td><%= link_to 'Show', story_state %></td>
    <td><%= link_to 'Edit', edit_story_state_path(story_state) %></td>
    <td><%= link_to 'Destroy', story_state, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New story_state', new_story_state_path %>