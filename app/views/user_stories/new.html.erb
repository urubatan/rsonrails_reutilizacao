<h1>New user_story</h1>

<% form_for(@user_story) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :project %><br />
    <%= f.text_field :project %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :as_a %><br />
    <%= f.text_field :as_a %>
  </p>
  <p>
    <%= f.label :i_want_to %><br />
    <%= f.text_field :i_want_to %>
  </p>
  <p>
    <%= f.label :so_that %><br />
    <%= f.text_field :so_that %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', user_stories_path %>